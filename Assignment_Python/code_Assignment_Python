    print("F5")
    f0()


def f6():
    print("F6")
    f0()


def rec_factorial(n):
    if n == 0 or n == 1:
        return 1
    return n * rec_factorial(n - 1)


def rec_fibonacci(n):
    if n <= 0:
        return 0
    if n == 1:
        return 1
    return rec_fibonacci(n - 1) + rec_fibonacci(n - 2)


def rec_sum_digits(n):
    if n == 0:
        return 0
    return n % 10 + rec_sum_digits(n // 10)


def rec_gcd(a, b):
    if b == 0:
        return a
    return rec_gcd(b, a % b)


class Student:
    def __init__(self, name, roll, marks):
        self.name = name
        self.roll = roll
        self.marks = marks

    def show(self):
        print(self.name, self.roll, self.marks)


class Complex:
    count = 0

    def __init__(self, real, imag):
        self.real = real
        self.imag = imag
        Complex.count += 1

    def show(self):
        print(str(self.real) + "+" + str(self.imag) + "i")


class Account:
    count = 0

    def __init__(self, name, acc_no, balance):
        self.name = name
        self.acc_no = acc_no
        self.balance = balance
        Account.count += 1

    def deposit(self, amt):
        self.balance += amt

    def withdraw(self, amt):
        if amt <= self.balance:
            self.balance -= amt

    def show(self):
        print(self.name, self.acc_no, self.balance)


class Car:
    count = 0

    def __init__(self, car_id, car_name):
        self.car_id = car_id
        self.car_name = car_name
        Car.count += 1

    def show(self):
        print(self.car_id, self.car_name)


