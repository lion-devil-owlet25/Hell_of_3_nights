a=int(input("Enter the age of Dip:-"))
b=int(input("Enter the age of Rahul:-"))
c=int(input("Enter the age of Tamal:-"))
if (a<b and a<c):
	print("Dip is youngest of Rahul and Tamal")
if (b<a and b<c):
	print("Rahul is youngest of Dip and Tamal")
if (c<a and c<b:
	print("Tamal is youngest of Dip and Rahul")
